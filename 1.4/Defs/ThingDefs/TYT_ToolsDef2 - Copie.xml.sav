<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ============================Blanket================================== -->
	<ThingDef ParentName="BaseTools_Forged">
		<defName>Tool_Blanket</defName>
		<label>blanket</label>
		<description>A comfy security blanket. Keeps you warm, and keeps you happy.</description>
		<graphicData>
			<texPath>Things/Item/Equipment/Tools/Blanket</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>100</costStuffCount>
		<statBases>
			<WorkToMake>1200</WorkToMake>
			<Mass>0.5</Mass>
		</statBases>
		<equippedStatOffsets>
			<ComfyTemperatureMin>-10</ComfyTemperatureMin>
		</equippedStatOffsets>
		<tools>
			<li>
				<label>sheet</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<cooldownTime>2</cooldownTime>
				<power>1</power>
			</li>
		</tools>
		<modExtensions>
			<li Class="TakeYourTools.TYT_ToolProperties">
				<baseWorkStatFactors>
					<ConstructionSpeed>1</ConstructionSpeed>
					<ConstructSuccessChance>1</ConstructSuccessChance>
					<MiningSpeed>1</MiningSpeed>
				</baseWorkStatFactors>
				<defaultToolAssignmentTags>
					<li>Clean</li>
					<li>Harvest</li>
				</defaultToolAssignmentTags>
			</li>
		</modExtensions>
	</ThingDef>
</Defs>